#include<stdio.h>
int main()
{
    int n;
    scanf("%d", &n);
    while (n--) {
        int i = 0,j = 0,s;
        char arr[100][100];
        scanf("%d", &s);
        for (; i < s; i++) scanf("%s", arr[i]);
        for (; j < s; j++) {
            for (i = s - 1; i >= 0; i--)printf("%c", arr[i][j]); printf("\n");
        }
        printf("\n");
    }
    return 0;
}
